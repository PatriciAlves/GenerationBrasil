package introducao;

import java.util.Scanner;

public class OlaMundo {
	
	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		int n1, n2;
		System.out.println("Digite o primeiro numero: ");
		n1=ler.nextInt();
		System.out.println("Digite o primeiro numero: ");
		n2=ler.nextInt();
		
		System.out.println("numeros digitados: " + n1 + n2);
	}

}
